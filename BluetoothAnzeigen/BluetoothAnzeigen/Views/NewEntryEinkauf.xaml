<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BluetoothAnzeigen.Views.NewEntryEinkauf"
             Title="New Entry">

    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Save" />
    </ContentPage.ToolbarItems>
    
    <ContentPage.Content>
        <TableView Intent="Form">
            <TableView.Root>
                <TableSection>
                    <EntryCell Label="Betrag" Keyboard="Numeric" />
                    <EntryCell Label="Marktname" />
                    <EntryCell Label="Datum" />
                    <EntryCell Label="Artikelanzahl" Keyboard="Numeric" />
                    <EntryCell Label="Latitude" Keyboard="Numeric" />
                    <EntryCell Label="Longitude" Keyboard="Numeric" />
                </TableSection>
            </TableView.Root>
        </TableView>
    </ContentPage.Content>
</ContentPage>